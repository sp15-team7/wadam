    "cn",
    "CODEIT",
    "colour",
    "JAEHYUN",
    "JINHYUNG",
    "oklch",
    "Pretendard",
    "SARANG",
    "shadcn",
    "SUMIN",
    "WHYNE",
    "wineid",
    "YUNGHYUN"
  ]
}
